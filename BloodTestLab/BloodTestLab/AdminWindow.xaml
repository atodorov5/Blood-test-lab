<Window x:Class="BloodTestLab.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BloodTestLab"
        mc:Ignorable="d"
        Title="Администраторски прозорец" Height="500" Width="600" Icon="/images/blood-test.png">
    <Grid >
        <StackPanel Height="25" VerticalAlignment="Top">
            <Menu HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="592">
                <MenuItem Header="Създаване на акаунт" FontSize="16" Click="MenuItem_Click"></MenuItem>
                <MenuItem Header="Добавяне на клиника" FontSize="16"></MenuItem>
            </Menu>
        </StackPanel>

        <Frame x:Name="adminMainFrame" Margin="0,25,0,0" Background="Coral" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
    </Window>
